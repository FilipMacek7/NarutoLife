<Page x:Class="NarutoLife.views.pages.IchirakuRamen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NarutoLife.views.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="IchirakuRamen">
    <Page.Background>
        <ImageBrush ImageSource="../../img/ichirakuramen.jpg" x:Name="background"></ImageBrush>
    </Page.Background>
    <Grid>
        <Frame x:Name="profile" Height="300" Width="200" Panel.ZIndex="3"></Frame>
        <Frame x:Name="profilebar" VerticalAlignment="Top" HorizontalAlignment="Left" Height="138" Width="350"></Frame>
        <Button Height="40" Width="80" Content="Enter" Click="EnterRamen" Name="enterbutton"></Button>
        <Frame x:Name="time" VerticalAlignment="Top" HorizontalAlignment="Right"></Frame>
        <Button Height="40" Width="80" Content="Village" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="10" Click="GoVillage"></Button>
    </Grid>
</Page>
