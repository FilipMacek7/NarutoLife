<Page x:Class="NarutoLife.views.frames.ListPanel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NarutoLife.views.frames"
      mc:Ignorable="d" 
      d:DesignHeight="50" d:DesignWidth="100"
      Title="ListPanel">

    <Grid>
        <StackPanel Grid.Column="5" HorizontalAlignment="Right" VerticalAlignment="Top" Orientation="Horizontal">
            <Button   Width="30" Height="30" Margin="10" Click="Settings_On">
                <StackPanel>
                    <Image Source="../../img/settings.png"/>
                </StackPanel>
            </Button>
            <Button  Width="30" Height="30" Margin="10" Click="Inventory_Open">
                <StackPanel>
                    <Image Source="../../img/inventory_icon.png"/>
                </StackPanel>
            </Button>
        </StackPanel>
    </Grid>
</Page>
